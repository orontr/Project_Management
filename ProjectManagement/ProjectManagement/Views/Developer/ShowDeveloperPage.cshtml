@using ProjectManagement.Models
@{
    ViewBag.Title = "ShowDeveloperPage";
}

<h4>שלום @(((User)(Session["CurrentUser"])).FirstName)</h4>

<div class="row">
    <div class="col-sm-offset-8">
        <button id="Message" class="btn btn-primary btn-block btn-lg" type="button" onclick="location.href='@Url.Action("MessagesPage")'">הודעות</button>
        <button id="CreateForm" class="btn btn-primary btn-block btn-lg" type="button" onclick="location.href='@Url.Action("CreateForm")'">צור מסמך </button>
        <button id="EditForms" class="btn btn-primary btn-block btn-lg" type="button" onclick="location.href='@Url.Action("ChooseForm")'">עריכת מסמך קיים </button>
       
    </div>
</div>

